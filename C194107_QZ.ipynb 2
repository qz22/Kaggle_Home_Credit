   ],
   "source": [
    "# Apply lgboost\n",
    "train_x = mdata_modeling[ X_cols ]\n",
    "train_y = mdata_modeling['Tip_percent']\n",
    "feat_impt_df = lightgbm_regressor( train_x, train_y  )"
   ]
  },
