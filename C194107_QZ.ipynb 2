   "source": [
    "# Remove trips with 'Trip_distance' greater than 20 miles, and build distribution / box plot again\n",
    "fig, (ax1, ax2) = plt.subplots( ncols=2 )\n",
    "sns.distplot( (mdata.loc[ mdata[ 'Trip_distance' ] < 20 ])['Trip_distance' ],\\\n",
